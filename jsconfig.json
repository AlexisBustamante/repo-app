{
  "vueCompilerOptions": {
    //"target": 2.7,
    "target": 2, // For Vue version <= 2.6.14
  },
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
     "extensions": [
    ".vue"
  ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
